{
    "root": true,
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-undef": [2],
        "no-unused-vars": [2, {
            "varsIgnorePattern": "React"
        }]
    }
}